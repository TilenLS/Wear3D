{
    "ShowLogs": true,
    "QMainWindow": [
        {
            "tittle": "Tooth Wear Decision Support System"
        }
    ],
    "QSettings": [
        {
            "ThemeSettings": [
                {
                    "CustomTheme": [
                        {
                            "Default-Theme": false
                        }
                    ]
                }
            ],
            "AppSettings": {
                "OrginizationName": "Tooth Wear",
                "ApplicationName": "Tooth Wear Company",
                "OrginizationDormain": "Tooth Wear.org"
            }
        }
    ],
    "QPushButtonGroup": [
        {
            "Buttons": [
                "homeButton",
                "analysisButton",
                "viewButton"
            ],
            "Style":[
                {
                    "Active": "background-color: #FFFFFF; font-weight: bold;",
                    "NotActive": "background-color: transparent; border: none"
                }
            ]
	    }
    ],
    "QCustomSlideMenu": [
        {
            "name": "leftMenu",
            "defaultSize": [
                {
                    "width": 200,
                    "height": "parent"
                }
            ],
            "collapsedSize": [
                {
                    "width": 0,
                    "height": "parent"
                }
            ],
            "expandedSize": [
                {
                    "width": 200,
                    "height": "parent"
                }
            ],
            "toggleButton": [
                {
                    "buttonName": "menuButton",
                    "icons": [
                        {
                            "whenMenuIsCollapsed": ":/icons/Icons/align-left.png",
                            "whenMenuIsExpanded": ":/icons/Icons/chevron-left.png"
                        }
                    ]
                }
            ],
            "menuTransitionAnimation": [
                {
                    "animationDuration": 500,
                    "animationEasingCurve": "Linear"
                }
            ]
	    },
        {
            "name": "rightMenu",
            "defaultSize": [
                {
                    "width": 0,
                    "height": "parent"
                }
            ],
            "collapsedSize": [
                {
                    "width": 0,
                    "height": "parent"
                }
            ],
            "expandedSize": [
                {
                    "width": 300,
                    "height": "parent"
                }
            ],
            "toggleButton": [
                {
                    "buttonName": "showPatientFormButton",
                    "icons": [
                        {
                            "whenMenuIsCollapsed": ":/icons/Icons/plus-square_focus.png",
                            "whenMenuIsExpanded": ":/icons/Icons/window_close_focus.png"
                        }
                    ]
                }
            ],
            "menuTransitionAnimation": [
                {
                    "animationDuration": 500,
                    "animationEasingCurve": "Linear"
                }
            ]
	    }
    ],
    "QStackedWidget":[
		{
			"name":"pages",
			"transitionAnimation":[
				{
					"fade":[
						{
							"active": false,
							"duration":1000,
							"easingCurve": "Linear"
						}
					],
					"slide":[
						{
							"active":true,
							"duration": 100,
							"direction": "horizontal",
							"easingCurve": "Linear"
						}
					]
				}
			],
			"navigation":[
				{
					"navigationButtons":[
						{
							"homeButton": "homePage",
							"viewButton": "viewPage",
                            "analysisButton": "analysisPage"
						}
					]
				}
			]
	
		}
	]
}